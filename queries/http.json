{
  "index": "<INDEX>",
  "type": "events",
  "from": 0,
  "size": 100,
  "body": {
    "query": {
      "bool": {
        "must": [
          { "match": { "event_type": "http"} }
        ],
        "must_not": [
          { "match": { "src_ip": "<SRC IP>"} },
          { "match": { "http.url": "/robots.txt"} },
          { "match": { "http.url": "/_search"} }
        ]
      }
    },
    "collapse": {
      "field": "http.url.keyword",
      "inner_hits": {
      "name": "latest",
        "size": 1
      }
    }
  }
}
